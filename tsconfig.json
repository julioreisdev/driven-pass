{
  "compilerOptions": {
    "outDir": "dist",
    "target": "es2016",                         
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,                                         
    "skipLibCheck": true         ,
    "build": "tsc && npx prisma generate && npx prisma migrate deploy"                        
  }
}
